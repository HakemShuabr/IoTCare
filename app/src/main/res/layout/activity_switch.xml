<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.firstversionapp.SwitchActivity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="375dp"
        android:layout_height="55dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

        <TextView
            android:id="@+id/fan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="101dp"
            android:layout_marginTop="8dp"
            android:text="fan"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@android:color/holo_red_light"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.47" />

        <Switch
            android:id="@+id/fanSwitch"
            style="@android:style/Widget.Material.CompoundButton.Switch"
            android:layout_width="44dp"
            android:layout_height="42dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="101dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:button="@android:color/transparent"
            android:textOff="off"
            android:textOn="on"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="375dp"
        android:layout_height="55dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="88dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

        <TextView
            android:id="@+id/ac"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="101dp"
            android:layout_marginTop="8dp"
            android:text="AC"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@android:color/holo_red_light"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.47" />

        <Switch
            android:id="@+id/acSwitch"
            style="@android:style/Widget.Material.CompoundButton.Switch"
            android:layout_width="44dp"
            android:layout_height="42dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="101dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:button="@android:color/transparent"
            android:textOff="off"
            android:textOn="on"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="375dp"
        android:layout_height="55dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        android:layout_marginTop="158dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

        <TextView
            android:id="@+id/door"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="101dp"
            android:layout_marginTop="8dp"
            android:text="door"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@android:color/holo_red_light"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.47" />

        <Switch
            android:id="@+id/doorSwitch"
            style="@android:style/Widget.Material.CompoundButton.Switch"
            android:layout_width="44dp"
            android:layout_height="42dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="101dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:button="@android:color/transparent"
            android:textOff="off"
            android:textOn="on"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="375dp"
        android:layout_height="55dp"
        android:id="@+id/constraintLayout4"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="103dp">

        <TextView
            android:id="@+id/light"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="light"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@android:color/holo_red_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="101dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.47"
            app:layout_constraintBottom_toBottomOf="parent" />

        <Switch
            android:id="@+id/lightSwitch"
            style="@android:style/Widget.Material.CompoundButton.Switch"
            android:layout_width="44dp"
            android:layout_height="42dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="101dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:button="@android:color/transparent"
            android:textColorLink="@android:color/holo_green_dark"
            android:textOff="off"
            android:textOn="on"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/back"
        android:layout_width="42dp"
        android:layout_height="34dp"
        android:layout_marginLeft="16dp"
        android:layout_weight="0.04"
        android:width="80dp"
        android:height="80dp"
        android:background="@mipmap/back1600"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

</android.support.constraint.ConstraintLayout>
